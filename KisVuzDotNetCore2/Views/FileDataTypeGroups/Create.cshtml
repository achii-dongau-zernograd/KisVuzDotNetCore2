@model KisVuzDotNetCore2.Models.FileDataTypeGroup

@{
    ViewData["Title"] = "Создание группы типов содержимого файлов";
}

<h2>Создание группы типов содержимого файлов</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FileDataTypeGroupName" class="control-label"></label>
                <input asp-for="FileDataTypeGroupName" class="form-control" />
                <span asp-validation-for="FileDataTypeGroupName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Создать" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
