@model IEnumerable<KisVuzDotNetCore2.Models.Users.TeacherMetodKomissiya>

@{
    ViewData["Title"] = "Преподаватель - Методкомиссия";
}

<h2>Преподаватель - Методкомиссия</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MetodKomissiya)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Teacher)
            </th>
            <th>
                Действия
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MetodKomissiya.MetodKomissiyaName)
            </td>
            <td>
                <a asp-controller="UserProfile" asp-action="Index" asp-route-id="@item.Teacher.AppUserId">@item.Teacher.TeacherFio</a>
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TeacherMetodKomissiyaId">Редактировать</a> |
                <a asp-action="Delete" asp-route-id="@item.TeacherMetodKomissiyaId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
