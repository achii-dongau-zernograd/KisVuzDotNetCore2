@model KisVuzDotNetCore2.Models.Users.TeacherMetodKomissiya

@{
    ViewData["Title"] = "Преподаватель - Методкомиссия";
}

<h2>Добавить</h2>

<h4>Преподаватель - Методкомиссия</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Teacher" class="control-label"></label>
                <select asp-for="TeacherId" class ="form-control" asp-items="ViewBag.TeacherId"></select>
            </div>
            <div class="form-group">
                <label asp-for="MetodKomissiya" class="control-label"></label>
                <select asp-for="MetodKomissiyaId" class ="form-control" asp-items="ViewBag.MetodKomissiyaId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>