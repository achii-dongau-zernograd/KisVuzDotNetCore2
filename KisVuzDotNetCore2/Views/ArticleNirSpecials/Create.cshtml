@model KisVuzDotNetCore2.Models.Nir.ArticleNirSpecial

@{
    ViewData["Title"] = "Добавить соответствие Научная статья - начная специальность";
}

<h2>Добавить соответствие Научная статья - начная специальность</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ArticleId" class="control-label"></label>
                <select asp-for="ArticleId" class ="form-control" asp-items="ViewBag.ArticleId"></select>
            </div>
            <div class="form-group">
                <label asp-for="NirSpecialId" class="control-label"></label>
                <select asp-for="NirSpecialId" class ="form-control" asp-items="ViewBag.NirSpecialId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>