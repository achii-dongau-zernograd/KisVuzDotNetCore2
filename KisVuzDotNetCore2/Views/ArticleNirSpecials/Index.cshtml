@model IEnumerable<KisVuzDotNetCore2.Models.Nir.ArticleNirSpecial>

@{
    ViewData["Title"] = "Научная статья - Научная специальность";
}

<h2>Научная статья - Научная специальность</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Article)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NirSpecial)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Article.ArticleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NirSpecial.NirSpecialName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ArticleNirSpecialId">Редактировать</a> |
                <a asp-action="Delete" asp-route-id="@item.ArticleNirSpecialId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
