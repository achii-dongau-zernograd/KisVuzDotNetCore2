@model KisVuzDotNetCore2.Models.EduGraduate

@{
    ViewData["Title"] = "Редактировать Выпуск (результаты защиты ВКР)";
}

<h2>Редактировать Выпуск (результаты защиты ВКР)</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EduGraduateId" />
            <div class="form-group">
                <label asp-for="EduProfile.EduProfileName" class="control-label"></label>
                <select asp-for="EduProfileId" class="form-control" asp-items="ViewBag.EduProfileId"></select>
                <span asp-validation-for="EduProfileId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GraduateYear.GraduateYearName" class="control-label"></label>
                <select asp-for="GraduateYearId" class="form-control" asp-items="ViewBag.GraduateYearId"></select>
                <span asp-validation-for="GraduateYearId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GraduateNumber" class="control-label"></label>
                <input asp-for="GraduateNumber" class="form-control" />
                <span asp-validation-for="GraduateNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GraduateSredBall" class="control-label"></label>
                <input asp-for="GraduateSredBall" class="form-control" />
                <span asp-validation-for="GraduateSredBall" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary"/>
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>