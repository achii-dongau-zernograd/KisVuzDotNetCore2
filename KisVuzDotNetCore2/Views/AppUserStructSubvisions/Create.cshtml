@model KisVuzDotNetCore2.Models.Users.AppUserStructSubvision

@{
    ViewData["Title"] = "Добавление";
}

<h2>Добавление</h2>

<h4>Пользователь - Структурное подразделение - Должность - Форма занятости</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AppUserId" class="control-label"></label>
                <select asp-for="AppUserId" class ="form-control" asp-items="ViewBag.AppUserId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StructSubvisionId" class="control-label"></label>
                <select asp-for="StructSubvisionId" class ="form-control" asp-items="ViewBag.StructSubvisionId"></select>
            </div>
            <div class="form-group">
                <label asp-for="PostId" class="control-label"></label>
                <select asp-for="PostId" class ="form-control" asp-items="ViewBag.PostId"></select>
            </div>
            <div class="form-group">
                <label asp-for="EmploymentFormId" class="control-label"></label>
                <select asp-for="EmploymentFormId" class ="form-control" asp-items="ViewBag.EmploymentFormId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
