@model KisVuzDotNetCore2.Models.Users.AppUserStructSubvision

@{
    ViewData["Title"] = "Редактирование";
}

<h2>Пользователь - Структурное подразделение - Должность - Форма занятости</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AppUserStructSubvisionId" />
            <div class="form-group">
                <label asp-for="AppUserId" class="control-label"></label>
                <select asp-for="AppUserId" class="form-control" asp-items="ViewBag.AppUserId"></select>
                <span asp-validation-for="AppUserId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StructSubvisionId" class="control-label"></label>
                <select asp-for="StructSubvisionId" class="form-control" asp-items="ViewBag.StructSubvisionId"></select>
                <span asp-validation-for="StructSubvisionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PostId" class="control-label"></label>
                <select asp-for="PostId" class="form-control" asp-items="ViewBag.PostId"></select>
                <span asp-validation-for="PostId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmploymentFormId" class="control-label"></label>
                <select asp-for="EmploymentFormId" class="form-control" asp-items="ViewBag.EmploymentFormId"></select>
                <span asp-validation-for="EmploymentFormId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
