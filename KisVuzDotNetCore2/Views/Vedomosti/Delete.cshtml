@model KisVuzDotNetCore2.Models.Students.Vedomost

@{
    ViewData["Title"] = "Удаление";
}

<h2>Удаление</h2>

<h3>Вы действительно хотите удалить запись?</h3>
<div>
    <h4>Ведомость</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.EduYear)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduYear.EduYearName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.StudentGroup)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.StudentGroup.StudentGroupName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.SemestrName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SemestrName.SemestrNameName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.DisciplineName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="VedomostId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-default">Отмена</a>
    </form>
</div>
