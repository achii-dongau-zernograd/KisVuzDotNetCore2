@model KisVuzDotNetCore2.Models.Students.Vedomost

@{
    ViewData["Title"] = "Добавление";
}

<h2>Добавление</h2>

<h4>Ведомость</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EduYearId" class="control-label"></label>
                <select asp-for="EduYearId" class ="form-control" asp-items="ViewBag.EduYearId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StudentGroupId" class="control-label"></label>
                <select asp-for="StudentGroupId" class ="form-control" asp-items="ViewBag.StudentGroupId"></select>
            </div>
            <div class="form-group">
                <label asp-for="SemestrNameId" class="control-label"></label>
                <select asp-for="SemestrNameId" class ="form-control" asp-items="ViewBag.SemestrNameId"></select>
            </div>
            <div class="form-group">
                <label asp-for="DisciplineName" class="control-label"></label>
                <input asp-for="DisciplineName" class="form-control" />
                <span asp-validation-for="DisciplineName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>