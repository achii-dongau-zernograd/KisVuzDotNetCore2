@model IEnumerable<KisVuzDotNetCore2.Models.Sveden.PomeshenieTypepomesheniya>

@{
    ViewData["Title"] = "Помещение - Тип помещения";
}

<h2>Помещение - Тип помещения</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pomeshenie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PomeshenieType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pomeshenie.PomeshenieName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PomeshenieType.PomeshenieTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PomeshenieTypepomesheniyaId">Редактировать</a> |                
                <a asp-action="Delete" asp-route-id="@item.PomeshenieTypepomesheniyaId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
