@model KisVuzDotNetCore2.Models.Sveden.PomeshenieTypepomesheniya

@{
    ViewData["Title"] = "Добавить тип помещения";
}

<h2>Добавить тип помещения</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Pomeshenie.PomeshenieName" class="control-label"></label>
                <select asp-for="PomeshenieId" class ="form-control" asp-items="ViewBag.PomeshenieId"></select>
            </div>
            <div class="form-group">
                <label asp-for="PomeshenieType.PomeshenieTypeName" class="control-label"></label>
                <select asp-for="PomeshenieTypeId" class ="form-control" asp-items="ViewBag.PomeshenieTypeId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
