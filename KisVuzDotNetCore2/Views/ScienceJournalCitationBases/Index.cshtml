@model IEnumerable<KisVuzDotNetCore2.Models.Nir.ScienceJournalCitationBase>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CitationBase)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScienceJournal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CitationBase.CitationBaseId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScienceJournal.ScienceJournalId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ScienceJournalCitationBaseId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ScienceJournalCitationBaseId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ScienceJournalCitationBaseId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
