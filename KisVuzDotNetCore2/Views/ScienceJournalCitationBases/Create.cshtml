@model KisVuzDotNetCore2.Models.Nir.ScienceJournalCitationBase

@{
    ViewData["Title"] = "Добавить соответствие Научный журнал - База Цитирования";
}

<h2>Добавить соответствие Научный журнал - База Цитирования</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ScienceJournalId" class="control-label"></label>
                <select asp-for="ScienceJournalId" class ="form-control" asp-items="ViewBag.ScienceJournalId"></select>
            </div>
            <div class="form-group">
                <label asp-for="CitationBaseId" class="control-label"></label>
                <select asp-for="CitationBaseId" class ="form-control" asp-items="ViewBag.CitationBaseId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>