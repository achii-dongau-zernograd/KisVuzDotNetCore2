@model IEnumerable<KisVuzDotNetCore2.Models.NirTemaEduProfile>

@{
    ViewData["Title"] = "Тематика НИР - профиль подготовки";
}

<h2>Тематика НИР - профиль подготовки</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EduProfile.GetEduProfileFullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NirTema.NirTemaName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EduProfile.GetEduProfileFullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NirTema.NirTemaName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NirTemaEduProfileId">Редактировать</a>
                <a asp-action="Details" asp-route-id="@item.NirTemaEduProfileId">Подробнее</a>
                <a asp-action="Delete" asp-route-id="@item.NirTemaEduProfileId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
