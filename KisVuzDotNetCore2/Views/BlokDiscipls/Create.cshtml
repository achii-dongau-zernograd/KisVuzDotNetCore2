@model KisVuzDotNetCore2.Models.Education.BlokDiscipl

@{
    ViewData["Title"] = "Добавить";
}

<h2>Добавить блок дисциплин</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="BlokDisciplName" class="control-label"></label>
                <select asp-for="BlokDisciplNameId" class ="form-control" asp-items="ViewBag.BlokDisciplNameId"></select>
            </div>
            <div class="form-group">
                <label asp-for="EduPlan" class="control-label"></label>
                <select asp-for="EduPlanId" class ="form-control" asp-items="ViewBag.EduPlanId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>



