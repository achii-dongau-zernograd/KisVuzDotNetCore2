@model KisVuzDotNetCore2.Models.Education.BlokDiscipl

@{
    ViewData["Title"] = "Удалить";
}


<h3>Удалить блок дисциплин?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.BlokDisciplName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.BlokDisciplName.BlokDisciplNameName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EduPlan)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduPlan.EduPlanDescription)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="BlokDisciplId" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-default">Отмена</a>
    </form>
</div>
