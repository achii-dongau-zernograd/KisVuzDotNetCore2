@model IEnumerable<KisVuzDotNetCore2.Models.Education.BlokDiscipl>

@{
    ViewData["Title"] = "Блоки дисциплин";
}

<h2>Блоки дисциплин</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BlokDisciplName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EduPlan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BlokDisciplName.BlokDisciplNameId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EduPlan.EduPlanId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BlokDisciplId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.BlokDisciplId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.BlokDisciplId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
