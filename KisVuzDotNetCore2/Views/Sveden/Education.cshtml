@{
    ViewData["Title"] = "Образование";

    var license = (IEnumerable<FileDataType>)ViewData["license"];
    var t9eduAccred = (IEnumerable<EduNapravlEduFormEduSrok>)ViewData["t9eduAccred"];
    var t7eduChislen = (IEnumerable<EduChislen>)ViewData["t7eduChislen"];
    var t8eduPriem = (IEnumerable<EduPriem>)ViewData["t8eduPriem"];
    var t9eduPerevod = (IEnumerable<EduPerevod>)ViewData["t9eduPerevod"];
    var t10eduPrograms = (IEnumerable<EduProgram>)ViewData["t10eduPrograms"];
    var eduShedules = (IEnumerable<EduShedule>)ViewData["eduShedules"];
    var eduPlans = (IEnumerable<EduPlan>)ViewData["eduPlans"];
    var t11eduOPYears = (IEnumerable<EduOPYear>)ViewData["t11eduOPYears"];
    var t11eduObrProg = (IEnumerable<EduPr>)ViewData["t11eduObrProg"];
    var t13eduNir = (IEnumerable<EduNir>)ViewData["t13eduNir"];

    int spoilerCounter = 1;

    string openedSpoiler = (string)ViewBag.OpenedSpoiler;
    string eduAccredClass = "collapse";
    string eduPOAccredClass = "collapse";
    string eduChislenClass = "collapse";
    string eduPriemClass = "collapse";
    string eduPerevodClass = "collapse";
    string eduProgramsClass = "collapse";
    string eduAdOpClass = "collapse";
    string eduOAccredClass = "collapse";
    string eduNirClass = "collapse";
    string eduDPOClass = "collapse";

    switch (openedSpoiler)
    {
        case "eduAccred":
            eduAccredClass = "collapse.in";
            break;
        case "eduPOAccred":
            eduPOAccredClass = "collapse.in";
            break;
        case "eduChislen":
            eduChislenClass = "collapse.in";
            break;
        case "eduPriem":
            eduPriemClass = "collapse.in";
            break;
        case "eduPerevod":
            eduPerevodClass = "collapse.in";
            break;
        case "eduPrograms":
            eduProgramsClass = "collapse.in";
            break;
        case "eduAdOp":
            eduAdOpClass = "collapse.in";
            break;
        case "eduOAccred":
            eduOAccredClass = "collapse.in";
            break;
        case "eduNir":
            eduNirClass = "collapse.in";
            break;
        case "eduDPO":
            eduDPOClass = "collapse.in";
            break;
        default:
            break;
    }
}


<h1>Образование</h1>


<div itemprop="copyDoc">
    @foreach (var fileDataType in license)
    {
        <a href="#spoiler-@spoilerCounter" data-toggle="collapse" class="btn btn-primary btn-block">@fileDataType.FileDataTypeName</a>
        <div class="collapse" id="spoiler-@spoilerCounter" itemprop="docLink">
            @{spoilerCounter++;}
            <div class="well">
                @foreach (var ftft in fileDataType.FileToFileTypes)
                {
                    <p itemprop="@ftft.FileDataType.Itemprop">
                        <a href="/@ftft.FileModel.Path">@ftft.FileModel.Name</a>
                    </p>
                }
            </div>
        </div>
    }
</div>


@if (openedSpoiler != "eduAccred" && !string.IsNullOrWhiteSpace(openedSpoiler))
{
    <a asp-route-openedSpoiler="eduAccred" class="btn btn-primary btn-block">
