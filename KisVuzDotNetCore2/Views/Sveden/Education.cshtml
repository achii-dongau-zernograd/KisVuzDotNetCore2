                @foreach (var eduProgram in t10eduPrograms.Where(op => !op.IsAdopt))
                {
                    string opMain = "opMain";
                    string educationPlan = "educationPlan";
                    string educationAnnotation = "educationAnnotation";
                    string educationShedule = "educationShedule";
                    string methodology = "methodology";
                    string eduEl = "eduEl";

