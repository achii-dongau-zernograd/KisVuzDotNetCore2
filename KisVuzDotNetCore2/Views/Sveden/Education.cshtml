                                    Открыть
                                </a>
                            </td>
                            <td itemprop="eduPrac">
                                <a asp-controller="Eios" asp-action="UchPlansRabProgs"
                                    asp-route-eduNapravlId="@item.EduNapravlId"
                                    asp-route-eduFormId="@item.EduFormId"
                                    target="_blank" class="btn btn-primary">
                                    Открыть
                                </a>
                            </td>
                            <td itemprop="eduEl"> Не используется </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
}



