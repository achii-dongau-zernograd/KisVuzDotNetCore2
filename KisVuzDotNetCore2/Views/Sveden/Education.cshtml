                    {
                        <tr itemprop="eduAccred">
                            <td itemprop="eduCode">@item.EduNapravl.EduNapravlCode</td>
                            <td itemprop="eduName">@item.EduNapravl.EduNapravlName</td>
                            <td itemprop="eduProf">
                                @if (item.EduNapravl.EduProfiles!=null)
                                {
                                    foreach (var eduProfile in item.EduNapravl.EduProfiles)
                                    {
                                        @eduProfile.EduProfileName
                                        <br />
                                    }
                                }
                                else
                                {
                                    <text>-</text>
                                }

                            </td>
                            <td itemprop="eduLevel">@item.EduNapravl.EduUgs.EduLevel.EduLevelName</td>
                            <td itemprop="eduForm">
                                @item.EduForm.EduFormName
                            </td>
                            <td itemprop="learningTerm">
                                @item.EduSrok.EduSrokName
                            </td>
