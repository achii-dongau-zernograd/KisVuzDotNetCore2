                                        @{spoilerCounter++;}
                                        <div class="well">
                                            @foreach (var eduPlan in eduPlans.Where(p => p.EduProfileId == eduProgram.EduProfileId))
                                            {
                                                <p><a asp-controller="EduPlans" asp-action="Preview" asp-route-id="@eduPlan.EduPlanId">Открыть (@eduPlan.EduForm.EduFormName)</a></p>
                                            }
                                        </div>
                                    </div>
                                </li>
