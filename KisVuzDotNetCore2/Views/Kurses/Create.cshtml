@model KisVuzDotNetCore2.Models.Education.Kurs

@{
    ViewData["Title"] = "Добавить";
}

<h2>Добавить курс</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EduKursId" class="control-label"></label>
                <select asp-for="EduKursId" class ="form-control" asp-items="ViewBag.EduKursId"></select>
            </div>
            <div class="form-group">
                <input type="submit" asp-route-DisciplineId="@ViewBag.DisciplineId" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" asp-route-DisciplineId="@ViewBag.DisciplineId" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>


