@model KisVuzDotNetCore2.Models.Education.SemestrFormKontrolName

@{
    ViewData["Title"] = "Удалить";
}

<h3>Удалить форму контроля?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.FormKontrolName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FormKontrolName.FormKontrolNameName)
        </dd>
        </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="SemestrFormKontrolNameId" />
        <input type="submit" asp-route-SemestrId="@ViewBag.SemestrId" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" asp-route-SemestrId="@ViewBag.SemestrId" class="btn btn-default">Отмена</a>
    </form>
</div>
