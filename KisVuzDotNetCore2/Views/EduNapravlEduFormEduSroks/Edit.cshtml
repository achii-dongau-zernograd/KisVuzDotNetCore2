@model KisVuzDotNetCore2.Models.Education.EduNapravlEduFormEduSrok

@{
    ViewData["Title"] = "Edit";
}

<h2>Редактировать информацию о направлении подготовки, форме обучения и нормативном сроке обучения</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EduNapravlEduFormEduSrokId" />
            <div class="form-group">
                <label asp-for="EduNapravlId" class="control-label"></label>
                <select asp-for="EduNapravlId" class="form-control" asp-items="ViewBag.EduNapravls"></select>
                <span asp-validation-for="EduNapravlId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EduFormId" class="control-label"></label>
                <select asp-for="EduFormId" class="form-control" asp-items="ViewBag.EduForms"></select>
                <span asp-validation-for="EduFormId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EduSrokId" class="control-label"></label>
                <select asp-for="EduSrokId" class="form-control" asp-items="ViewBag.EduSroks"></select>
                <span asp-validation-for="EduSrokId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>