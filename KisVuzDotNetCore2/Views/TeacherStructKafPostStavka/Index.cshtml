@model IEnumerable<KisVuzDotNetCore2.Models.Users.TeacherStructKafPostStavka>

@{
    ViewData["Title"] = "Преподаватель - Кафедра - Должность - Ставка - Дата установления ставки";
}

<h2>Преподаватель - Кафедра - Должность - Ставка - Дата установления ставки</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Teacher)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StructKaf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Post)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StavkaDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stavka)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmploymentForm)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Teacher.TeacherFio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StructKaf.StructSubvision.StructSubvisionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Post.PostName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StavkaDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stavka)
            </td>
            <td>
                @item.EmploymentForm?.EmploymentFormName
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TeacherStructKafPostStavkaId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.TeacherStructKafPostStavkaId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.TeacherStructKafPostStavkaId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
