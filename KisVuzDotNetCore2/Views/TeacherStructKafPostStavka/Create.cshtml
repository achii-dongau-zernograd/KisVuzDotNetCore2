@model KisVuzDotNetCore2.Models.Users.TeacherStructKafPostStavka

@{
    ViewData["Title"] = "Добавление";
}

<h2>Добавление</h2>

<h4>Преподаватель - Кафедра - Должность - Ставка - Дата установления ставки</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TeacherId" class="control-label"></label>
                <select asp-for="TeacherId" class="form-control" asp-items="ViewBag.TeacherId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StructKafId" class="control-label"></label>
                <select asp-for="StructKafId" class="form-control" asp-items="ViewBag.StructKafId"></select>
            </div>
            <div class="form-group">
                <label asp-for="PostId" class="control-label"></label>
                <select asp-for="PostId" class="form-control" asp-items="ViewBag.PostId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StavkaDate" class="control-label"></label>
                <input asp-for="StavkaDate" class="form-control" />
                <span asp-validation-for="StavkaDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Stavka" class="control-label"></label>
                <input asp-for="Stavka" class="form-control" />
                <span asp-validation-for="Stavka" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmploymentFormId" class="control-label"></label>
                <select asp-for="EmploymentFormId" class="form-control" asp-items="ViewBag.EmploymentForms"></select>
                <span asp-validation-for="EmploymentFormId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>
