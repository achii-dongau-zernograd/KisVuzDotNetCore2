@model KisVuzDotNetCore2.Models.Abitur.EduNapravlQuotaType

@{
    ViewData["Title"] = "Edit";
}

<h2>Редактирование</h2>

<h4>Направление подготовки - Тип квоты приёма</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EduNapravlQuotaTypeId" />
            <div class="form-group">
                <label asp-for="EduNapravlId" class="control-label"></label>
                <select asp-for="EduNapravlId" class="form-control" asp-items="ViewBag.EduNapravlId"></select>
                <span asp-validation-for="EduNapravlId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QuotaTypeId" class="control-label"></label>
                <select asp-for="QuotaTypeId" class="form-control" asp-items="ViewBag.QuotaTypeId"></select>
                <span asp-validation-for="QuotaTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>