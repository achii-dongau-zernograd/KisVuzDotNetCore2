@model IEnumerable<KisVuzDotNetCore2.Models.priemKolMest>

@{
    ViewData["Title"] = "Информация о количестве мест для приема на обучение по различным условиям поступления";
}

<h2>Информация о количестве мест для приема на обучение по различным условиям поступления</h2>

<p>
    <a asp-action="Create">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
        <tr>
            <td rowspan="2">Код</td>
            <td rowspan="2">Направление подготовки(специальности)</td>
            <td rowspan="2">Всего</td>
            <td colspan="6" align="center">Контрольные цифры приёма</td>
            <td colspan="3"> По договорам о показании платных образовательных услуг</td>

        </tr>
        <tr>
            <td colspan="3" align="center">Особая квота</td>
            <td colspan="3" align="center">Общие условия</td>


        <tr>
            <td></td>
            <td></td>
            <td></td>
            <td>Очное</td>
            <td>Заочное</td>
            <td>Очно-заочное</td>
            <td>Очное</td>
            <td>Заочное</td>
            <td>Очно-заочное</td>
            <td>Очное</td>
            <td>Заочное</td>
            <td>Очно-заочное</td>
        </tr>


        <tr>
            <td>1</td>
            <td>2</td>
            <td>3</td>
            <td>4</td>
            <td>5</td>
            <td>6</td>
            <td>7</td>
            <td>8</td>
            <td>9</td>
            <td>10</td>
            <td>11</td>
            <td>12</td>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EduNapravl.EduNapravlCode</td>
                <td>@item.EduNapravl.EduNapravlName</td>
                <td>@item.PriemKolMestAll</td>
                <td>
                    @Html.DisplayFor(modelItem => item.priemKolMestQuota_och)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.priemKolMestQuota_zaoch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.priemKolMestQuota_och_zaoch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestCommon_och)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestCommon_zaoch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestCommon_och_zaoch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestPaid_och)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestPaid_zaoch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PriemKolMestPaid_och_zaoch)
                </td>

                <td>
                    <a asp-action="Edit" asp-route-id="@item.priemKolMestId">Редактировать</a>
                    <a asp-action="Details" asp-route-id="@item.priemKolMestId">Подробнее</a>
                    <a asp-action="Delete" asp-route-id="@item.priemKolMestId">Удалить</a>
                </td>
            </tr>
        }
    </tbody>
</table>
