@model KisVuzDotNetCore2.Models.Qualification

@{
    ViewData["Title"] = "Удаление";
}

<h2>Удаление</h2>

<h3>Вы действительно хотите удалить запись?</h3>
<div>
    <h4>Квалификация</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.AppUser)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AppUser.GetFullName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.NapravlName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NapravlName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.QualificationName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.QualificationName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="QualificationId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-default">Отмена</a>
    </form>
</div>
