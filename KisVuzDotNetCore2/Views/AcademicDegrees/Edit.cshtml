@model KisVuzDotNetCore2.Models.AcademicDegree

@{
    ViewData["Title"] = "Редактировать учёную степень";
}

<h2>Редактировать учёную степень</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AcademicDegreeId" />
            <div class="form-group">
                <label asp-for="AcademicDegreeName" class="control-label"></label>
                <input asp-for="AcademicDegreeName" class="form-control" />
                <span asp-validation-for="AcademicDegreeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AcademicDegreeGroup.AcademicDegreeGroupName" class="control-label"></label>
                <select asp-for="AcademicDegreeGroupId" class="form-control" asp-items="ViewBag.AcademicDegreeGroupId"></select>
                <span asp-validation-for="AcademicDegreeGroupId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
