@model KisVuzDotNetCore2.Models.AcademicDegree

@{
    ViewData["Title"] = "Добавить учёную степень";
}

<h2>Добавить учёную степень</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AcademicDegreeName" class="control-label"></label>
                <input asp-for="AcademicDegreeName" class="form-control" />
                <span asp-validation-for="AcademicDegreeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AcademicDegreeGroup.AcademicDegreeGroupName" class="control-label"></label>
                <select asp-for="AcademicDegreeGroupId" class ="form-control" asp-items="ViewBag.AcademicDegreeGroupId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Создать" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
