@model IEnumerable<KisVuzDotNetCore2.Models.AcademicDegree>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.AcademicDegreeName)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.AcademicDegreeGroup)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AcademicDegreeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcademicDegreeGroup.AcademicDegreeGroupId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AcademicDegreeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AcademicDegreeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AcademicDegreeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
