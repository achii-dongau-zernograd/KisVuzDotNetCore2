@model IEnumerable<KisVuzDotNetCore2.Models.Education.BlokDisciplChast>

@{
    ViewData["Title"] = "Части блоков дисциплин";
}

<h2>Части блоков дисциплин</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BlokDisciplChastName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BlokDisciplChastName.BlokDisciplChastNameName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BlokDisciplChastId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.BlokDisciplChastId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.BlokDisciplChastId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
