@model KisVuzDotNetCore2.Models.Education.BlokDisciplChast

@{
    ViewData["Title"] = "Удалить";
}

<h3>Удалить часть блока дисциплин?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.BlokDiscipl)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.BlokDiscipl.BlokDisciplName.BlokDisciplNameName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.BlokDisciplChastName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.BlokDisciplChastName.BlokDisciplChastNameName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="BlokDisciplChastId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" asp-route-id="@ViewBag.BlokDisciplId" class="btn btn-default">Отмена</a>
    </form>
</div>
