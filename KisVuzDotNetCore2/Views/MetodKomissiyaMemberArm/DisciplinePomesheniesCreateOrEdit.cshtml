@model DisciplinePomeshenie

<h4>Дисциплина - Помещение</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="DisciplinePomesheniesCreateOrEditConfirmed">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DisciplinePomeshenieId" />
            <input type="hidden" asp-for="DisciplineId" />
            <input type="hidden" asp-for="EduPlanEduYearId" />
            <div class="form-group">
                <label class="control-label">Учебный год: @Model.EduPlanEduYear.EduYear.EduYearName</label>
            </div>
            <div class="form-group">
                <label class="control-label">Дисциплина: @Model.Discipline.DisciplineName.DisciplineNameName</label>
            </div>
            <div class="form-group">
                <label asp-for="PomeshenieId" class="control-label"></label>
                <select asp-for="PomeshenieId" class="form-control" asp-items="ViewBag.Pomesheniya"></select>
                <span asp-validation-for="PomeshenieId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" name="EduPlanId" value="@ViewBag.EduPlanId" />
                <input type="hidden" name="EduYearId" value="@Model.EduPlanEduYear.EduYearId" />
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="EduPlanPreview" asp-route-id="@ViewBag.EduPlanId" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>