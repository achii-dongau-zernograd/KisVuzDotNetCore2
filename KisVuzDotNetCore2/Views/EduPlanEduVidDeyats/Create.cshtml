@model KisVuzDotNetCore2.Models.Education.EduPlanEduVidDeyat

@{
    ViewData["Title"] = "Добавить";
}

<h2>Добавить вид деятельности к учебному плану</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EduPlanId" class="control-label"></label>
                <select asp-for="EduPlanId" class ="form-control" asp-items="ViewBag.EduPlanId"></select>
            </div>
            <div class="form-group">
                <label asp-for="EduVidDeyat.EduVidDeyatName" class="control-label"></label>
                <select asp-for="EduVidDeyatId" class="form-control" asp-items="ViewBag.EduVidDeyatId"></select>
                <span asp-validation-for="EduVidDeyat" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>



