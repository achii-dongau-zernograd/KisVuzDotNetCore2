@model IEnumerable<KisVuzDotNetCore2.Models.Education.EduPlanEduVidDeyat>

@{
    ViewData["Title"] = "Вид деятельности по учебному плану";
}

<h2>Вид деятельности по учебному плану</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EduPlan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EduVidDeyat)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EduPlan.EduPlanDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EduVidDeyat.EduVidDeyatName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EduPlanEduVidDeyatId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.EduPlanEduVidDeyatId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.EduPlanEduVidDeyatId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
