@model KisVuzDotNetCore2.Models.Education.Discipline

@{
    ViewData["Title"] = "Редактировать";
}

<h2>Редактировать</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DisciplineId" />
            <div class="form-group">
                <label asp-for="DisciplineCode" class="control-label"></label>
                <input asp-for="DisciplineCode" class="form-control" />
                <span asp-validation-for="DisciplineCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DisciplineNameId" class="control-label"></label>
                <select asp-for="DisciplineNameId" class="form-control" asp-items="ViewBag.DisciplineNameId"></select>
                <span asp-validation-for="DisciplineNameId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" name="BlokDisciplChastId" value="@ViewBag.BlokDisciplChastId" />

            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" asp-route-BlokDisciplChastId="@ViewBag.BlokDisciplChastId" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>


