@model IEnumerable<KisVuzDotNetCore2.Models.Abitur.EduNapravlEduForm>

@{
    ViewData["Title"] = "Index";
}

<h2>Направления подготовки - Формы обучения</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EduNapravlId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EduFormId)
            </th>
            <th>Действия</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EduNapravl.GetEduNapravlName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EduForm.EduFormName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EduNapravlEduFormId" class="btn btn-primary">Редактировать</a>
                <a asp-action="Delete" asp-route-id="@item.EduNapravlEduFormId" class="btn btn-danger">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
