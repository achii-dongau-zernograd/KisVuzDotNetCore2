@model KisVuzDotNetCore2.Models.Priem.PriemExam

@{
    ViewData["Title"] = "Редактирование";
}

<h2>Редактирование</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PriemExamId" />
            <div class="form-group">
                <label asp-for="EduNapravlId" class="control-label"></label>
                <select asp-for="EduNapravlId" class="form-control" asp-items="ViewBag.EduNapravlId"></select>
                <span asp-validation-for="EduNapravlId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VstupIsp" class="control-label"></label>
                <input asp-for="VstupIsp" class="form-control" />
                <span asp-validation-for="VstupIsp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MinKol" class="control-label"></label>
                <input asp-for="MinKol" class="form-control" />
                <span asp-validation-for="MinKol" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FormProv" class="control-label"></label>
                <input asp-for="FormProv" class="form-control" />
                <span asp-validation-for="FormProv" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>



