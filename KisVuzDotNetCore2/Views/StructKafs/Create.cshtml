@model KisVuzDotNetCore2.Models.Struct.StructKaf

@{
    ViewData["Title"] = "Добавить кафедру";
}

<h2>Добавить кафедру</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StructFacultetId" class="control-label"></label>
                <select asp-for="StructFacultetId" class ="form-control" asp-items="ViewBag.StructFacultetId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StructSubvisionId" class="control-label"></label>
                <select asp-for="StructSubvisionId" class ="form-control" asp-items="ViewBag.StructSubvisionId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
