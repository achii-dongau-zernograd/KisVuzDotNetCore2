@model KisVuzDotNetCore2.Models.Struct.StructKaf

@{
    ViewData["Title"] = "Редактировать";
}

<h2>Редактировать</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="StructKafId" />
            <div class="form-group">
                <label asp-for="StructFacultet.StructFacultetId" class="control-label"></label>
                <select asp-for="StructFacultetId" class="form-control" asp-items="ViewBag.StructFacultetId"></select>
                <span asp-validation-for="StructFacultetId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StructSubvision.StructSubvisionName" class="control-label"></label>
                <select asp-for="StructSubvisionId" class="form-control" asp-items="ViewBag.StructSubvisionId"></select>
                <span asp-validation-for="StructSubvisionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
