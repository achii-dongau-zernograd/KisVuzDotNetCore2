@model IEnumerable<KisVuzDotNetCore2.Models.Sveden.ElectronBiblSystem>

@{
    ViewData["Title"] = "Электронные библиотечные системы";
}

<h2>Электронные библиотечные системы</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameEbs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberDogovor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateEnd)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a target="_blank" href="@item.LinkEbs">@item.NameEbs</a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberDogovor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateEnd)
            </td>            
         </tr>
}
    </tbody>
</table>
