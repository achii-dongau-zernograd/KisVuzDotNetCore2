@model KisVuzDotNetCore2.Models.Education.EduProgram

@{
    ViewData["Title"] = "Детализация";
}

<h2>Детализация</h2>

<div>
    <h4>Образовательная программа</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.EduProfile)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduProfile.GetEduProfileFullName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EduProgramPodg)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduProgramPodg.EduProgramPodgName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EduProgramEduForms)
        </dt>
        <dd>
            @foreach (var item in Model.EduProgramEduForms)
            {
                <p>@item.EduForm.EduFormName</p>
            }
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.EduProgramEduYears)
        </dt>
        <dd>
            @foreach (var item in Model.EduProgramEduYears)
            {
                <p>@item.EduYear.EduYearName</p>
            }
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.DateUtverjd)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DateUtverjd)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IsAdopt)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IsAdopt)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.UsingElAndDistEduTech)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UsingElAndDistEduTech)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.FileModel)
        </dt>
        <dd>
            <a href="/@Model.FileModel.Path">Открыть</a>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.EduProgramId" class="btn btn-default">Редактировать</a>
    <a asp-action="Index" class="btn btn-primary">Отмена</a>
</div>
