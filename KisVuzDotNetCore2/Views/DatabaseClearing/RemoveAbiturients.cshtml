@model List<Abiturient>

@{
}
<h1>Личные кабинеты указанных абитуриентов будут удалены!</h1>

<ol>
    @foreach (var abiturient in Model)
    {
        <li>
            @if(abiturient.AppUser==null)
            {
                <text>AppUser is NULL!</text>
            }
            else
            {
                @abiturient.AppUser.GetFullName
            }
            
            
            ЛК абитуриента создан: 
            @if(abiturient.RegisterDateTime==null)
            {
                <text>NULL</text>
            }
            else
            {
                @abiturient.RegisterDateTime
            }
        </li>
    }
</ol>

<div>
    <form asp-action="RemoveAbiturientsConfirmed">
        <input type="submit" value="Подтвердить удаление" class="btn btn-danger" />
    </form>
</div>