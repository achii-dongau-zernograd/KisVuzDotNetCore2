@model KisVuzDotNetCore2.Models.Education.EduProgramEduForm

@{
    ViewData["Title"] = "Добавление";
}

<h2>Добавление</h2>

<h4>Образовательная программа - форма обучения</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EduProgramId" class="control-label"></label>
                <select asp-for="EduProgramId" class ="form-control" asp-items="ViewBag.EduProgramId"></select>
            </div>
            <div class="form-group">
                <label asp-for="EduFormId" class="control-label"></label>
                <select asp-for="EduFormId" class ="form-control" asp-items="ViewBag.EduFormId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>