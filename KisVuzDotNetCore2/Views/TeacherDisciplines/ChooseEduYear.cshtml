
@{
    ViewData["Title"] = "ChooseEduYear";
}

<h2>Выберите учебный год</h2>

<form asp-action="ChooseEduYear">
    <div class="form-group">
        <label class="control-label">Учебный год</label>
        <select name="EduYearId" class="form-control" asp-items="ViewBag.EduYears"></select>
    </div>
    <div class="form-group">
        <input type="submit" value="Выбрать" class="btn btn-primary" />
        <a asp-controller="Home" asp-action="Index" class="btn btn-default">Отмена</a>
    </div>
</form>

