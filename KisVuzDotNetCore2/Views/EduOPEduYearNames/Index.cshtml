@model IEnumerable<KisVuzDotNetCore2.Models.Sveden.EduOPEduYearName>

@{
    ViewData["Title"] = "Справочник годов обучения";
}

<h2>Справочник годов обучения</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EduOPEduYearNameName)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EduOPEduYearNameName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EduOPEduYearNameId">Редактировать</a> |                
                <a asp-action="Delete" asp-route-id="@item.EduOPEduYearNameId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
