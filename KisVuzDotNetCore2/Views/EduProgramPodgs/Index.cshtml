@model IEnumerable<KisVuzDotNetCore2.Models.Education.EduProgramPodg>

@{
    ViewData["Title"] = "Программы подготовки";
}

<h2>Программы подготовки</h2>

<p>
    <a asp-action="Create" class=" btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EduProgramPodgName)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EduProgramPodgName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EduProgramPodgId">Редактировать</a> |
                <a asp-action="Delete" asp-route-id="@item.EduProgramPodgId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
