@model IEnumerable<KisVuzDotNetCore2.Models.Struct.MetodKomissiya>

@{
    ViewData["Title"] = "Перечень методкомиссий";
}

<h2>Перечень методкомиссий</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить методкомиссию</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MetodKomissiyaName)
            </th>
            <th>
                Действия
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MetodKomissiyaName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MetodKomissiyaId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.MetodKomissiyaId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.MetodKomissiyaId" class="text-danger">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
