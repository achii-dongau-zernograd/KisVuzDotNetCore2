@model KisVuzDotNetCore2.Models.Education.DisciplinePomeshenie

@{
    ViewData["Title"] = "Удаление";
}

<h2>Удаление</h2>

<h3>Вы действительно хотите удалить запись?</h3>
<div>
    <h4>Закрепление помещения за дисциплиной в учебном году</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.EduPlanEduYear.EduYear)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduPlanEduYear.EduYear.EduYearName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Discipline)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Discipline.DisciplineName.DisciplineNameName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Pomeshenie)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pomeshenie.PomeshenieFullName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="DisciplinePomeshenieId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" asp-route-EduPlanId="@Model.EduPlanEduYear.EduPlanId" asp-route-EduYearId="@Model.EduPlanEduYear.EduYearId" class="btn btn-default">Отмена</a>
    </form>
</div>
