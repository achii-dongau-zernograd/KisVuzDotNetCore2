@model IEnumerable<KisVuzDotNetCore2.Models.Nir.Year>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.YearName)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.YearName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.YearId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.YearId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.YearId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
