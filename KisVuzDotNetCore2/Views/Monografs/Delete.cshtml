@model KisVuzDotNetCore2.Models.Nir.Monograf

@{
    ViewData["Title"] = "Удалить сведения о монографии";
}

<h3>Удалить сведения о монографии?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.MonografName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MonografName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IsACHII)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IsACHII)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.FileModel)
