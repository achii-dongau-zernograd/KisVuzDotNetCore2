@model IEnumerable<KisVuzDotNetCore2.Models.Sveden.HostelInfo>

@{
    ViewData["Title"] = "Наличие общежития, интерната, количество жилых помещений в общежитии, интернате для иногородних обучающихся";
}

<h2>Наличие общежития, интерната, количество жилых помещений в общежитии, интернате для иногородних обучающихся</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NameIndicator)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Itemprop)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Value)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Link)
                </th>
            <th>Действие</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameIndicator)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Itemprop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Link)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Редактировать</a>
                <a asp-action="Delete" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
