@model KisVuzDotNetCore2.Models.Struct.MetodKomissiyaEduProfile

@{
    ViewData["Title"] = "Редактировать";
}

<h2>Редактировать</h2>

<h4>Методкомиссия - Профиль подготовки</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MetodKomissiyaEduProfileId" />
            <div class="form-group">
                <label asp-for="MetodKomissiya" class="control-label"></label>
                <select asp-for="MetodKomissiyaId" class="form-control" asp-items="ViewBag.MetodKomissiyaId"></select>
                <span asp-validation-for="MetodKomissiyaId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EduProfile" class="control-label"></label>
                <select asp-for="EduProfileId" class="form-control" asp-items="ViewBag.EduProfileId"></select>
                <span asp-validation-for="EduProfileId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>