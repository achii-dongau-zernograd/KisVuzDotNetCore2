@model KisVuzDotNetCore2.Models.Struct.MetodKomissiyaEduProfile

@{
    ViewData["Title"] = "Удаление";
}

<h2>Удаление</h2>

<h3>Вы действительно хотите удалить запись?</h3>
<div>
    <h4>Методкомиссия - Профиль подготовки</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.EduProfile)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EduProfile.GetEduProfileFullName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.MetodKomissiya)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MetodKomissiya.MetodKomissiyaName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="MetodKomissiyaEduProfileId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-default">Отмена</a>
    </form>
</div>
