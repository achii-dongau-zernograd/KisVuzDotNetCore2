@model KisVuzDotNetCore2.Models.Nir.NirSpecial

@{
    ViewData["Title"] = "Добавление сведений о специальности научных работников";
}

<h2>Добавление сведений о специальности научных работников</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NirSpecialCode" class="control-label"></label>
                <input asp-for="NirSpecialCode" class="form-control" />
                <span asp-validation-for="NirSpecialCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NirSpecialName" class="control-label"></label>
                <input asp-for="NirSpecialName" class="form-control" />
                <span asp-validation-for="NirSpecialName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>


