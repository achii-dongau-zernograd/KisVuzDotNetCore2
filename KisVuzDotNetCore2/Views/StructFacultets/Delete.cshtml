@model KisVuzDotNetCore2.Models.Struct.StructFacultet

@{
    ViewData["Title"] = "Удалить";
}

<h2>Удалить факультет?</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.StructFacultetName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.StructFacultetName)
        </dd>
        
        <dt>
            @Html.DisplayNameFor(model => model.StructInstitute.StructInstituteName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.StructInstitute.StructInstituteName)
        </dd>
        
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="StructFacultetId" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a class="btn btn-default" asp-action="Index">Отмена</a>
    </form>
</div>
