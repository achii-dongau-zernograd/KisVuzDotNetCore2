@model KisVuzDotNetCore2.Models.Struct.StructFacultet

@{
    ViewData["Title"] = "Добавить факультет";
}

<h2>Добавить факультет</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StructInstitute.StructInstituteName" class="control-label"></label>
                <select asp-for="StructInstituteId" class ="form-control" asp-items="ViewBag.StructInstituteId"></select>
            </div>
            <div class="form-group">
                <label asp-for="StructSubvision.StructSubvisionName" class="control-label"></label>
                <select asp-for="StructSubvisionId" class ="form-control" asp-items="ViewBag.StructSubvisionId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-default">Отмена</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
