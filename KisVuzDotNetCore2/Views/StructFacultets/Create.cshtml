@model KisVuzDotNetCore2.Models.Struct.StructFacultet

@{
    ViewData["Title"] = "Факультет. Добавить";
}

<h2>Добавить</h2>

<h4>Факультет</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StructFacultetName" class="control-label"></label>
                <input asp-for="StructFacultetName" class="form-control" />
                <span asp-validation-for="StructFacultetName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StructInstitute.StructInstituteName" class="control-label"></label>
                <select asp-for="StructInstituteId" class="form-control" asp-items="ViewBag.StructInstituteId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
                <a class="btn btn-default" asp-action="Index">Отмена</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
