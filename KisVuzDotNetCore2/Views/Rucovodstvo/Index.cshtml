@model IEnumerable<KisVuzDotNetCore2.Models.Sveden.Rucovodstvo>

@{
    ViewData["Title"] = "Руководство института";
}

<h2>Руководство института</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Fio)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Post)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Telephone)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>

                <th> Действия</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Post)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telephone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RucovodstvoId">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.RucovodstvoId">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.RucovodstvoId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
