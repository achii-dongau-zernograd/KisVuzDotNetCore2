@model IEnumerable<KisVuzDotNetCore2.Models.Struct.InstituteLink>

@{
    ViewData["Title"] = "Index";
}

<h2>Веб-ресурсы института</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.InstituteLinkLink)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.InstituteLinkDescription)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.LinkType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StructInstitute)
            </th>
            <th>
                Действия
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InstituteLinkLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstituteLinkDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LinkType.LinkTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StructInstitute.StructInstituteName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InstituteLinkId">Редактировать</a>
                <a asp-action="Delete" asp-route-id="@item.InstituteLinkId">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
